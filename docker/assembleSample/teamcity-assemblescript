docker --version

file_id="cid"
apkDir="./apk"
containertag="androidcontainer"

cd ../../

docker build -t $containertag -f docker/assembleSample/Dockerfile .
docker run --cidfile $file_id $containertag

container_id=$(cat $file_id)
echo $container_id

mkdir $apkDir

docker cp $container_id:/VerificationSampleLegacy/build/outputs/apk/debug $apkDir

docker container rm $container_id
rm $file_id
